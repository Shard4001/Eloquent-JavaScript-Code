/*
FizzBuzz
Determines which values from 1 to 10 are divisible by both 3 and 5 and each of them as well 
Chapter 2 - Exercise 2
*/

for(let i = 1; i <= 100; i++) {
    if (i % 3 === 0 && i % 5 === 0 ) {
      console.log("FizzBuzz");
    
    }
    else if (i % 3 === 0) {
      console.log("Fizz")
    }
    else if (i % 5 === 0) {
      console.log("Buzz")
    }
    
  else {
      console.log(i);
  }
    }
